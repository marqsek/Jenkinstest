    agent any
    
    stages {
        stage("init") {
            steps {
                script {
                   gv = load "script.groovy" 
                }
            }
        }
        stage("build") {
            steps {
				echo 'building the app'
                }
            }
        }
        stage("test") {
            steps {
				echo 'building the app'
                }
            }
        }
        stage("deploy") {
            steps {
				echo 'building the app'
                }
        }
    }   
}
